<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/preview.jpeg</argument>
                <argument name="logo_width" xsi:type="number">150</argument>
                <argument name="logo_height" xsi:type="number">150</argument>
                <argument name="logo_alt" xsi:type="string">Logo PatBrands</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>